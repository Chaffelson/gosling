{
    "display_information": {
        "name": "Gosling",
        "description": "Knowledgebase Bot",
        "background_color": "#335c41",
        "long_description": "Gosling is our helpful internal Knowledgebase chat Bot. It knows about our Docs and can help with most information-retrieval questions. You can DM Gosling or @ it in a configured channel, or summon it privately with /honk."
    },
    "features": {
        "app_home": {
            "home_tab_enabled": false,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "Gosling",
            "always_online": true
        },
        "slash_commands": [
            {
                "command": "/honk",
                "url": "replaceme",
                "description": "Summon the Gosling.",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "app_mentions:read",
                "channels:history",
                "channels:read",
                "chat:write",
                "chat:write.public",
                "commands",
                "groups:history",
                "groups:read",
                "im:history",
                "im:read",
                "im:write",
                "mpim:history",
                "mpim:read",
                "mpim:write",
                "users:read",
                "reactions:read"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "request_url": "replaceme",
            "bot_events": [
                "app_mention",
                "message.channels",
                "message.groups",
                "message.im",
                "message.mpim",
                "reaction_added",
                "reaction_removed"
            ]
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}